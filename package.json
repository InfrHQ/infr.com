{
    "name": "infr",
    "version": "0.1.2",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "eslint 'app/**/*.{js,jsx,ts,tsx}' && prettier --write .",
        "format:check": "prettier --check 'app/**/*.{js,jsx,ts,tsx,css,md,json,html}'",
        "format": "prettier --write 'app/**/*.{js,jsx,ts,tsx,css,md,json,html}'",
        "prepare": "husky install"
    },
    "lint-staged": {
        "*.{js,jsx}": [
            "eslint --fix",
            "git add"
        ],
        "*.{js,jsx,css,md,json,html}": [
            "prettier --write",
            "git add"
        ]
    },
    "dependencies": {
        "@mdx-js/loader": "^2.3.0",
        "@mdx-js/react": "^2.3.0",
        "@next/mdx": "^13.4.13",
        "@radix-ui/react-icons": "^1.3.0",
        "autoprefixer": "10.4.14",
        "flowbite-typography": "^1.0.3",
        "next": "^13.4.19",
        "next-themes": "^0.2.1",
        "openai": "^3.3.0",
        "postcss": "8.4.27",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-player": "^2.12.0",
        "react-terminal": "^1.3.1",
        "tailwindcss": "3.3.3"
    },
    "devDependencies": {
        "eslint": "^8.47.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "husky": "^8.0.0",
        "lint-staged": "^14.0.1",
        "prettier": "^3.0.1"
    }
}
